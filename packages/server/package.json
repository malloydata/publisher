{
  "name": "@malloy-publisher/server",
  "description": "Malloy Publisher Server",
  "version": "0.0.105",
  "main": "dist/server.js",
  "bin": {
    "malloy-publisher": "dist/server.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "bun test --timeout 100000",
    "build": "bun generate-api-types && bun build:app && NODE_ENV=production bun run build.ts",
    "build:server-only": "bun generate-api-types && NODE_ENV=production bun run build.ts",
    "start": "NODE_ENV=production node ./dist/server.js",
    "start:dev": "NODE_ENV=development bun --watch src/server.ts",
    "start:instrumented": "node --require ./dist/instrumentation.js ./dist/server.js",
    "lint": "bunx eslint \"./src/**/*.ts\"",
    "format": "bunx prettier --write '**/*.{ts,tsx}'",
    "build:app": "cd ../app && NODE_ENV=production bunx vite build",
    "generate-api-types": "bunx openapi-typescript ../../api-doc.yaml --output src/api.ts"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.16.0",
    "@malloydata/db-bigquery": "^0.0.307",
    "@malloydata/db-duckdb": "^0.0.307",
    "@malloydata/db-mysql": "^0.0.307",
    "@malloydata/db-postgres": "^0.0.307",
    "@malloydata/db-snowflake": "^0.0.307",
    "@malloydata/db-trino": "^0.0.307",
    "@malloydata/malloy": "^0.0.307",
    "@malloydata/malloy-sql": "^0.0.307",
    "@malloydata/render": "^0.0.307",
    "@modelcontextprotocol/sdk": "^1.13.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.57.0",
    "@opentelemetry/sdk-metrics": "^2.0.0",
    "@opentelemetry/sdk-node": "^0.200.0",
    "@opentelemetry/sdk-trace-node": "^2.0.0",
    "adm-zip": "^0.5.16",
    "async-mutex": "^0.5.0",
    "aws-sdk": "^2.1692.0",
    "body-parser": "^1.20.2",
    "chokidar": "^4.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "globals": "^15.9.0",
    "handlebars": "^4.7.8",
    "http-proxy-middleware": "^3.0.5",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "recursive-readdir": "^2.2.3",
    "simple-git": "^3.28.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@anthropic-ai/dxt": "^0.1.0",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@faker-js/faker": "^9.4.0",
    "@types/adm-zip": "^0.5.7",
    "@types/bun": "^1.2.20",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.9",
    "@types/node-cron": "^3.0.11",
    "@types/recursive-readdir": "^2.2.4",
    "@types/sinon": "^17.0.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "mock-aws-s3": "^4.0.2",
    "nock": "^14.0.1",
    "openapi-typescript": "^6.7.6",
    "supertest": "^7.0.0"
  }
}