{
  "name": "@malloy-publisher/server",
  "description": "Malloy Publisher Server",
  "version": "0.0.1",
  "main": "dist/server.js",
  "bin": {
    "publisher": "dist/server.js"
  },
  "scripts": {
    "test": "mocha --exit",
    "build": "tsc",
    "start": "tsc && tsx ./dist/server.js",
    "lint": "tsc && eslint ./src",
    "format": "prettier --write --parser typescript '**/*.{ts,tsx}'",
    "generate-api-types": "openapi-typescript ../../api-doc.yaml --output src/api.ts",
    "build-duckdb-wasm": "tsx scripts/build-duckdb-wasm"
  },
  "dependencies": {
    "@malloydata/db-bigquery": "^0.0.240",
    "@malloydata/db-duckdb": "^0.0.240",
    "@malloydata/db-postgres": "^0.0.240",
    "@malloydata/db-snowflake": "^0.0.240",
    "@malloydata/db-trino": "^0.0.240",
    "@malloydata/malloy": "^0.0.240",
    "@malloydata/malloy-sql": "^0.0.240",
    "@malloydata/render": "^0.0.240",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "globals": "^15.9.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "recursive-readdir": "^2.2.3",
    "tsx": "^4.10.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.4.0",
    "@types/supertest": "^6.0.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^18.11.11",
    "@types/node-cron": "^3.0.11",
    "@types/recursive-readdir": "^2.2.4",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "supertest": "^7.0.0"
  }
}
