{
  "name": "@malloy-publisher/server",
  "description": "Malloy Publisher Server",
  "version": "0.0.1",
  "main": "dist/server.js",
  "bin": {
    "publisher": "dist/server.js"
  },
  "scripts": {
    "test": "bun test",
    "build": "bun build src/server.ts src/instrumentation.ts --outdir dist --target bun",
    "start": "bun run ./dist/server.js",
    "start:instrumented": "bun run --preload ./dist/instrumentation.js ./dist/server.js",
    "lint": "bunx eslint \"./src/**/*.ts\"",
    "format": "bunx prettier --write '**/*.{ts,tsx}'",
    "generate-api-types": "bunx openapi-typescript ../../api-doc.yaml --output src/api.ts"
  },
  "dependencies": {
    "@malloydata/db-bigquery": "^0.0.240",
    "@malloydata/db-duckdb": "^0.0.240",
    "@malloydata/db-postgres": "^0.0.240",
    "@malloydata/db-snowflake": "^0.0.240",
    "@malloydata/db-trino": "^0.0.240",
    "@malloydata/malloy": "^0.0.240",
    "@malloydata/malloy-sql": "^0.0.240",
    "@malloydata/render": "^0.0.240",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.57.0",
    "@opentelemetry/sdk-metrics": "^2.0.0",
    "@opentelemetry/sdk-node": "^0.200.0",
    "@opentelemetry/sdk-trace-node": "^2.0.0",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "globals": "^15.9.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "recursive-readdir": "^2.2.3",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.4.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.9",
    "@types/node-cron": "^3.0.11",
    "@types/recursive-readdir": "^2.2.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "aws-sdk": "^2.1692.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "mock-aws-s3": "^4.0.2",
    "nock": "^14.0.1",
    "openapi-typescript": "^6.7.6",
    "supertest": "^7.0.0"
  }
}
